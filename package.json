{
  "name": "boardseed",
  "version": "0.0.1",
  "description": "Seed project for full-stack Elm application based on Board ",
  "main": "local.js",
  "scripts": {
    "start": "nodemon --exec \"npm run back && npm run front && node local.js\"",
    "front": "elm make src/front/app.elm --output public/app.js",
    "back": "elm make src/back/app.elm --output=dist/app.js",
    "install": "elm-install install",
    "add": "elm package install",
    "elm": "elm"
  },
  "author": "airtucha",
  "license": "MIT",
  "dependencies": {
    "elm": "^0.18.0",
    "object-hash": "^1.3.1"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "elm-github-install": "^1.6.1"
  }
}
